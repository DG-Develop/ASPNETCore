@model Escuela

@{
    ViewData["Title"] = "Información Escuela";
    Layout = "Simple";
}

<h1>Escuela @ViewData["Title"]</h1>
<h2>Nombre: @Model.Nombre </h2>
<h3>Tipo de Escuela: @Model.TipoEscuela</h3>
<address>
    <P>Dirección: @Model.Direccion</P>
    <P>País: @Model.Pais</P>
    <P>Ciudad: @Model.Ciudad</P>
</address>
<p><strong>Año fundación: @Model.AnioCreacion</strong></p>

<p><i>@ViewBag.CosaDinamica</i></p>